pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	grid = {
		{6,6,6,6,6,6,6,6},
		{6,0,0,0,6,0,0,6},
		{6,0,0,0,6,0,0,6},
		{6,0,0,0,6,0,0,6},
		{6,0,0,0,0,0,0,6},
		{6,0,6,6,6,0,0,6},
		{6,0,0,0,0,0,0,6},
		{6,6,6,6,6,6,6,6}
	}
	p = {}
	p.x = 3
	p.y = 3
	p.spd = 0.1
	p.alpha = -0.25
	p.alpha_spd = 0.01
end

function _draw()
 cls()
 draw_walls()
 minimap(3)
 print(p.alpha,64,64,8)
end

function minimap(size)
 rectfill(0,0,size*8-1,size*8-1,5)
 for y=1,8 do
  for x=1,8 do
   if (grid[y][x] != 0) rectfill((x-1)*size,(y-1)*size,(x-1)*size+size-1,(y-1)*size+size-1,6)
  end
 end
 circfill((p.x-1)*size,(p.y-1)*size,size/2,12)
end

function _update()
 movement()
end

function walk(mult)
 local pos = {x=p.x,y=p.y}
 pos.x += cos(p.alpha) * p.spd * mult
 pos.y += sin(p.alpha) * p.spd * mult
 if (grid[flr(pos.y)][flr(pos.x)] == 0) then
  p.x = pos.x
  p.y = pos.y
 end
end

function movement()
 if (btn(0)) p.alpha += p.alpha_spd
 if (btn(1)) p.alpha -= p.alpha_spd
 if (btn(2)) walk(1)
 if (btn(3)) walk(-1)
end

-------------------------------
-----------raycasting----------
-------------------------------

function draw_walls()
 v = {}
 v.x = cos(p.alpha)
 v.y = sin(p.alpha)

	step = {}
	-- lorsque x+1, y+step.x
	step.x = {}
	if (v.x > 0) then
	 step.x[x] = 1
	 step.x[y] = sqrt(1+v.y^2/v.x^2)
	else
	 step.x[x] = -1
	 step.x[y] = sqrt(1+v.y^2/v.x^2)	
	end

 closer = {}
 if (v.x >= 0) then
	 closer.x[x] = flr(x)+1
	 closer.x[y] = --
	else
	 closer.x[x] = flr(x)
	 closer.x[y] = --
	end
end

function pythagore(a,b)
 return sqrt((b.x-a.x)^2+(b.y-a.y)^2)
end
