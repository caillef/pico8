pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	srand(time())
	p = {x=64-8,y=48}
	p.goleft = 0;
	p.cooldown = time()
	bullets = {};
	flip_pl = false
	s = {}
	spawn_time = 2
	last_spawn = time()
	particles = {}
	
	_update = g_update
	_draw = g_draw
end

function g_draw()
 cls()
 map(0,0,0,16,128,64)
 draw_player()
 draw_slimes()
 draw_bullet()
 draw_particles()
end

function draw_particles()
	for k,v in pairs(particles) do
	 spr(32+v.state,v.x,v.y)
	 v.state += 1
	 if (v.state > 4) del(particles,v)
	end
end

function draw_player()
 flip_pl = false
 if (p.goleft == 1) flip_pl=true
 sspr(p.sp*8,0,8,8,p.x,p.y,16,16,flip_pl)
end

function draw_slimes()
 for k,sli in pairs(s) do
  if (sli.dead == -1) then
	  sspr(0,8,8,8,sli.x,sli.y,16,16,sli.flp)
	 else
	  sspr(8*(flr(sli.dead/2)),8,8,8,sli.x,sli.y,16,16,sli.flp)
			sli.dead += 1
			if (sli.dead/2 > 4)	del(s,sli)
	 end
  if (sli.dead == -1) then
	  if (sli.flp) then
	   sli.x -= sli.speed
	  else
	   sli.x += sli.speed
	  end
  end
 end
end

function g_update()
 p.sp=0;
	controls()
	spawn_slime()
end

function spawn_slime()
 if (time() >= last_spawn) then
  slime = {
   x = flr(rnd(2)) * 159 - 16,
   y = p.y
  }
  slime.dead = -1
  slime.flp = true
  if (slime.x <= 0) slime.flp = false
  slime.speed = (rnd(2)/4)+1
  add(s,slime)
  last_spawn += spawn_time
  spawn_time = rnd(2)+1
 end
end

function explode(x,y)
 add(particles,{state=0,x=x-4,y=y-4})
end

function attack()
 if (time() >= p.cooldown) then
	 p.sp=6
	 sfx(0)
	 bullet = {orient=p.goleft*-2+1}
	 if (p.goleft==1)bullet.x=p.x-4
	 if (p.goleft==0)bullet.x=p.x+19
	 bullet.y=p.y+10
	 bullet.col=8
  add(bullets,bullet)
	 p.cooldown = time() + 0.3
	end
end

function draw_bullet()
 for k,v in pairs(bullets) do
  if (v.x < 0 or v.x > 127) del(bullets,v)
  for k,sli in pairs(s) do
   if (v.x >= sli.x and v.x <= sli.x+16) then
    sli.dead = 0
    sfx(1)
    explode(v.x-v.orient,v.y)
    del(bullets,v)
   end
  end
  if (v) then
		 pset(v.x,v.y,v.col)
		 pset(v.x-v.orient,v.y,v.col)
		 pset(v.x-2*v.orient,v.y,v.col)
		 v.x += v.orient*2;
		end
 end
end

function controls()
 if (btn(4)) attack()
 if (btn(0)) then
  p.x -= 1
  p.goleft = 1
 end
 if (btn(1)) then
  p.x += 1
  p.goleft = 0
 end
 if (btnp(5)) then
  _update = u_invent
  _draw = d_invent
 end
end

-----------------------

function u_invent()
 if (btnp(5)) then
  _update = g_update
  _draw = g_draw
 end
end

function d_invent()
 rectfill(0,0,128,128,4)
 
end
