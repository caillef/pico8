pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
particles = {}
explosion = {}
mouse = {
 x=64,
 y=64
}

function make_particles(nb)
 while (nb >= 0) do
	 p = {
	  x=mouse.x,
	  y=mouse.y,
	  vecx=rnd(4)-2,
	  vecy=rnd(4)-2,
	  c=flr(rnd(16)),
	  f=0,
	  maxf=30
	 }
	 add(particles,p)
	 nb -= 1
	end
end

function draw_particles()
 for pix in all(particles) do
  pset(pix.x,pix.y,pix.c)
  pix.x += pix.vecx
  pix.y += pix.vecy
  pix.f += 1
  if (pix.f >= pix.maxf
  or pix.x < 0 or pix.x >= 128
  or pix.y < 0 or pix.y >= 128) then
   del(particles,pix)
  end
 end
end

function draw_explosion()
 for e in all(explosion) do
  circfill(e.x,e.y,flr(e.r),e.c)
  e.r += e.sp
  if (e.r >= 10) then del(explosion,e)
  elseif (e.r >= 4) then e.c = 10
  elseif (e.r >= 2) then e.c = 9 end
 end
end

function _draw()
 cls()
 draw_particles()
 draw_explosion()
 spr(1,mouse.x-4,mouse.y-4)
end

function explode(nb)
	while (nb >= 0) do
	 e = {
	  x=mouse.x+rnd(20)-10,
	  y=mouse.y+rnd(20)-10,
	  c=8,
	  r=1,
	  sp=rnd(1)+1
	 }
	 add(explosion,e)
	 sfx(0)
	 nb -= 1
	end
end

function _update()
 if (btn(0)) mouse.x -= 1
 if (btn(1)) mouse.x += 1
 if (btn(2)) mouse.y -= 1
 if (btn(3)) mouse.y += 1
 if (btn(4)) make_particles(10)
 if (btnp(5)) explode(10)
end
